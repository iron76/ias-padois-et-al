	Issn = {0340-1200},
	Journal = {Biological cybernetics},
	Keywords = {Cybernetics,Humans,Locomotion,Locomotion: physiology,Models,Neurological,Neurosciences,Posture,Robotics},
	Month = mar,
	Number = {2},
