	Issn = {0079-6123},
	Journal = {Progress in brain research},
	Keywords = {Biological,Humans,Models,Postural Balance,Posture,Proprioception,Proprioception: physiology},
	Month = jan,
	Pages = {283--97},
	Pmid = {17925253},
	Title = {{Modeling sensorimotor control of human upright stance.}},
